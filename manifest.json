{
  "manifest_version": 3,
  "name": "Web Data Extractor Version 3.0",
  "version": "1.0",
  "description": "Logs semantic information about visited web pages",
  "icons": {
    "16": "./logo.png",
    "48": "./logo.png",
    "128": "./logo.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "downloads"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "history": "history.html"
  }
}